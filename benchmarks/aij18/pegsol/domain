;; Peg Solitaire sequential domain

(define (domain pegsolitaire-sequential)
    (:requirements)
    (:predicates
        (IN-LINE ?x ?y ?z)
        (occupied ?l)
        (free ?l)
        (move-ended)
        (last-visited ?l)
    )

    (:action jump-new-move
     :parameters (?o1 ?o2 ?o3)
     :precondition ()
     :effect ())

    (:action jump-continue-move
     :parameters (?o1 ?o2 ?o3)
     :precondition ()
     :effect ())

    (:action end-move
     :parameters (?o1)
     :precondition ()
     :effect ())
)