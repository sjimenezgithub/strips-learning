(define (domain ferry)
(:types car location - object)
   (:predicates (not-eq ?x ?y - object)
		(at-ferry ?l - location)
		(at ?c - car ?l - location)
		(empty-ferry)
		(on ?c - car))

   (:action sail
       :parameters  (?o1 - location ?o2 - location)
       :precondition ()
       :effect ())

   (:action board
       :parameters (?o1 - car ?o2 - location)
       :precondition  ()
       :effect ())

   (:action debark
       :parameters  (?o1 - car ?o2 - location)
       :precondition  ()
       :effect ())
)
